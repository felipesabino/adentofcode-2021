{
  "name": "adventofcode-2021",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "tslint 'sources/**/*.ts'",
    "test": "yarn run lint && jest --colors --verbose"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/node": "^16.11.11",
    "ts-node": "^3.3.0",
    "tslint": "^5.5.0",
    "typescript": "^2.4.2",
    "yarn": "^1.22.17"
  },
  "devDependencies": {
    "@types/jest": "^20.0.5",
    "jest": "^20.0.4",
    "ts-jest": "^20.0.7"
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "./node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/*test.ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/problems/template/.*"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
